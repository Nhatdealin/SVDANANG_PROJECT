<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme1" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/transperent</item>
        <item name="colorPrimaryDark">@color/transperent</item>
        <item name="colorAccent">@color/transperent</item>
        <item name="android:shadowColor">@color/transperent</item>
        <item name="android:outlineSpotShadowColor">@color/transperent</item>
        <item name="android:outlineAmbientShadowColor">@color/transperent</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
